                        {block:isDeactivated}<div class="deactiv">{/block:isDeactivated}
                        {Username}
                        {block:isDeactivated}</div>{/block:isDeactivated}
                    {block:HasPermalink}</a>{/block:HasPermalink}
                </div>
     
