p.tmblr-attribution a {
    text-decoration:none;
}

#credit {
	position:fixed;
	bottom:10px;
	right:10px;
	text-align:center;
	font-size:0.7em;
	line-height:110%;
	text-transform:uppercase;
	padding:5px;
}

#credit a {
	text-decoration:none;
	font-size:1.3em;
	font-weight:bold;
	font-family:{select:accent font};
}

.statlist {
    margin-bottom:20px;
}

.stat {
    width:100%;
    height:1em;
    margin-bottom:7px;
    border-bottom:1px dotted var(--borders);
}

.stat strong {
    float:left;
    padding-right:2px;
	background:var(--highlight);
	font-weight:bold;
    color:{color:accent text};
	letter-spacing:1px;
	text-transform:uppercase;
}

.stat em {
    float:right;
    padding-left:2px;
	background:var(--highlight);
	font-style:normal;
}

.listitem {
    min-height:100px;
    margin-bottom:30px;
}

.listitem img {
    float:left;
    width:100px;
    height:100px;
    object-fit:cover;
    margin-right:15px;
    margin-bottom:5px;
    padding:5px;
	border:1px solid var(--borders);
	background:var(--highlight);
}

.listitem h2 {
    padding-top:10px;
}

.listitem h3 {
    color:{color:accent text};
    font-weight:bold;
    text-transform:uppercase;
    font-size:0.9em;
    letter-spacing:1px;
    margin:-10px 0 15px 0;
    padding-left:0;
    border:none;
}

.listitem .listlinks {
    margin-top:-10px;
    margin-bottom:15px;
}

.listitem .listlinks a {
    display:inline-block;
    text-transform:uppercase;
    letter-spacing:1px;
    font-size:0.9em;
    text-decoration:none;
}

.listitem .listlinks a:after {
    content: " \2022 ";
}

.listitem .listlinks a:last-of-type:after {
    content: "";
}

.tagslist, .blogroll {
    vertical-align:top;
    column-count:2;
    margin-bottom:15px;
}

.tagslist ul, .blogroll ul {
    margin:0;
}

.tagslist li, .blogroll li {
    display:inline-block;
    width:150px;
    text-align:left;
    margin-left:-15px;
}

.tagslist li:before {
    content: "# ";
    margin-right:5px;
}

.blogroll li:before {
    content: "@ ";
    margin-right:5px;
}

{CustomCSS}
   	 
</style>
