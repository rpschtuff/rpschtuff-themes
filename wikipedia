    width:300px;
    background:#F8F9FA;
    border:1px solid #A2A9B1;
    padding:5px;
}

input[type=checkbox] {
    position:fixed;
    top:-9999px;
    left:-9999px;
}

label {
    -webkit-appearance: push-button;
    -moz-appearance: button; 
    display: inline-block;
    margin:0;
    cursor: pointer;
}

#navlist {
    display:inline;
}

input[type=checkbox]:checked ~ #navlist {
    display:none;
}

#contents ol {
    counter-reset:item1;
}

#contents ol li {
    display:block;
    font-size:0.9em;
    margin:3px 0 0 -30px;
}

#contents ol li:before {
    content:counter(item1) " ";
    counter-increment:item1;
    color:#000;
}

#contents ol ol {
    counter-reset:item2;
}

#contents ol ol li {
    margin-left:-50px;
}

#contents ol ol li:before {
    content:counter(item1)"."counter(item2) " ";
    counter-increment:item2;
}

.main {
    font-style:italic;
    margin: 10px 0 0 25px;
}

